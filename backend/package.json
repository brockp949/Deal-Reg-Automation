{
  "name": "dealreg-backend",
  "version": "1.0.0",
  "description": "Deal Registration Automation - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "worker": "tsx watch src/workers/index.ts",
    "db:migrate": "node -r tsx/cjs src/db/migrate.ts",
    "db:seed": "node -r tsx/cjs src/db/seed.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "source:sync": "node -r tsx/cjs src/scripts/runSourceSync.ts",
    "source:sync:gmail": "node -r tsx/cjs src/scripts/runSourceSync.ts --connector gmail",
    "source:sync:drive": "node -r tsx/cjs src/scripts/runSourceSync.ts --connector drive",
    "source:sync:crm-csv": "node -r tsx/cjs src/scripts/runSourceSync.ts --connector crm-csv",
    "source:sync:teams": "node -r tsx/cjs src/scripts/runSourceSync.ts --connector teams",
    "source:sync:zoom": "node -r tsx/cjs src/scripts/runSourceSync.ts --connector zoom",
    "source:process": "node -r tsx/cjs src/scripts/processSourceManifest.ts",
    "source:show": "node -r tsx/cjs src/scripts/showOpportunities.ts",
    "source:metrics": "node -r tsx/cjs src/scripts/opportunityMetrics.ts",
    "source:report": "node -r tsx/cjs src/scripts/opportunityReport.ts --publish docs/OPPORTUNITY_READINESS.md",
    "source:consolidate": "node -r tsx/cjs src/scripts/consolidateOpportunities.ts --log",
    "source:export": "node -r tsx/cjs src/scripts/exportCompositeOpportunities.ts",
    "source:quality": "node -r tsx/cjs src/scripts/opportunityQuality.ts",
    "source:feedback": "node -r tsx/cjs src/scripts/feedbackManager.ts",
    "source:publish": "node -r tsx/cjs src/scripts/publishDashboard.ts --publish docs/DASHBOARD.md",
    "source:history": "node -r tsx/cjs src/scripts/historyQuery.ts --limit 5",
    "source:ci": "npm run source:process && npm run source:export && npm run source:quality && npm run source:report && npm run source:publish",
    "smoke:gmail": "node -r tsx/cjs src/scripts/smokeTests.ts --connector gmail",
    "smoke:drive": "node -r tsx/cjs src/scripts/smokeTests.ts --connector drive",
    "smoke:crm-csv": "node -r tsx/cjs src/scripts/smokeTests.ts --connector crm-csv",
    "smoke:teams": "node -r tsx/cjs src/scripts/smokeTests.ts --connector teams",
    "smoke:zoom": "node -r tsx/cjs src/scripts/smokeTests.ts --connector zoom",
    "smoke:all": "node -r tsx/cjs src/scripts/smokeTests.ts --all",
    "load:test": "node -r tsx/cjs src/scripts/loadTest.ts",
    "insights:score": "node -r tsx/cjs src/scripts/insightsScore.ts",
    "insights:notify": "node -r tsx/cjs src/scripts/insightsNotify.ts",
    "insights:test": "node -r tsx/cjs src/scripts/notificationSelfTest.ts",
    "api:start": "npm start",
    "bulk:upload": "node -r tsx/cjs src/scripts/bulkUploadTestFiles.ts"
  },
  "keywords": [
    "deal-registration",
    "crm",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/html-to-text": "^9.0.4",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.1",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "fuzzball": "^2.2.3",
    "googleapis": "^166.0.0",
    "helmet": "^7.1.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "string-similarity": "^4.0.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mailparser": "^3.4.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/string-similarity": "^4.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
